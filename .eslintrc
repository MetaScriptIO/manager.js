{
	"env":
	{
		"browser": true
	},

	"rules":
	{
		"no-warning-comments":
		[
			1,
			{
				"terms": ["todo", "fixme", "bug"]
			}
		],

		"no-multiple-empty-lines":
		[
			2,
			{
				"max": 3
			}
		],

		"max-len":
		[
			2,
			{
				"code": 200
			}
		],

		"space-before-function-paren":
		[
			2,
			"never"
		],

		"comma-dangle":
		[
			2,
			"never"
		],

		"indent":
		[
			"error",
			"tab"
		],

		"keyword-spacing":
		[
			2,
			{
				"overrides":
				{
					"if":
					{
						"after": false
					},

					"catch":
					{
						"before": false,
						"after": false
					},

					"for":
					{
						"after": false
					},

					"else":
					{
						"before": false,
						"after": false
					}
				}
			}
		],

		"object-curly-spacing":
		[
			2,
			"never"
		],

		"semi":
		[
			2,
			"always"
		],

		"func-names": 0,
		"linebreak-style": 0,
		"no-tabs": 0,
		"brace-style": 0,
		"space-in-parens": 0,
		"camelcase": 0,
		"arrow-body-style": 0,
		"no-underscore-dangle": 0,
		"spaced-comment": 0
	}
}
